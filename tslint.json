{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": false,
        "quotemark": [
            true,
            "single"
        ],
        "one-line": [
            true,
            "check-open-brace",
            "check-whitespace"
        ],
        "no-bitwise": false,
        "object-literal-key-quotes": [
            true,
            "as-needed"
        ]
    },
    "rulesDirectory": []
}